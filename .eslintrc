{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "func-names": 2,
    "no-alert": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "strict": 0
  },
  "globals": {
    "afterEach": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "it": true,
    "jest": true,
    "pit": true,
    "xdescribe": true,
    "xit": true
  }
}

{
  "preset": "airbnb",
  "esnext": true,
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "plugins": [
    "jscs-jsdoc"
  ],
  "jsDoc": {
    "checkAnnotations": "jsdoc3",
    "checkParamNames": true,
    "checkRedundantParams": true,
    "checkRedundantAccess": true
  },
  "requireTrailingComma": false,
  "validateLineBreaks": false
}
